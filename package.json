	"main": "test.js",
	"scripts": {
		"build": "npx babel src --out-dir lib",
		"test": "cross-env NODE_ENV=test npm run unmigrate && cross-env NODE_ENV=test npm run migrate && cross-env NODE_ENV=test nyc --reporter=text --reporter=html  mocha --recursive --require @babel/polyfill --require @babel/register ./src/test/*.js --timeout 30000 --exit",
		"coverage": "nyc  --reporter=text-lcov --reporter=lcov npm run test | coveralls",
		"start": "cross-env NODE_ENV=production npm run dbmigrate && node lib/index.js",
		"dev": "nodemon --exec npx babel-node src/index.js",
		"start-lint": "eslint **/*.js",
		"lint": "eslint -r *.js",
		"migrate": "npx sequelize-cli db:migrate",
		"unmigrate": "npx sequelize-cli db:migrate:undo:all",
		"dbseed": "npx sequelize-cli db:seed:all",
		"dbseed:undo": "npx sequelize-cli db:seed:undo",
		"dbseed:undo:all": "npx sequelize-cli db:seed:undo:all"
	},
